CPP      = g++.exe
CC       = gcc.exe
WINDRES  = windres.exe
RES      = _private.res
OBJ      = main.o $(RES)
LINKOBJ  = main.o _private.res
CLEANOBJ  =  main.o  _private.res .exe
LIBS     = 
INCS     =  
CXXINCS  =  
BIN      = .exe
CXXFLAGS = $(CXXINCS)  -g3 -w -pipe -Wall -D__DEBUG__
CFLAGS   = $(INCS)  -g3 -w -pipe -Wall -D__DEBUG__
RM       = del /q /f
WINDRESFLAGS  = 

.PHONY: all all-before all-after clean clean-custom

all: all-before $(BIN) all-after

clean: clean-custom
	-$(RM) $(CLEANOBJ) > NUL 2>&1

$(BIN): $(OBJ)
	$(CPP) $(LINKOBJ) -o $(BIN) $(LIBS)

main.o: main.cpp select.h radix.h merge.h insert.h hanshu.h swap.h
	$(CPP) -c main.cpp -o main.o $(CXXFLAGS)  -finput-charset=UTF-8 -fexec-charset=gbk

_private.res: _private.rc 
	$(WINDRES) -i _private.rc --input-format=rc -o _private.res -O coff $(WINDRESFLAGS) 

